{
  "name": "mofresh",
  "version": "1.0.0",
  "description": "MoFresh Backend API",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx prisma generate && npx prisma migrate deploy && (node dist/main.js || node dist/src/main.js)",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start:railway": "npx prisma migrate resolve --rolled-back \"20260209105135_add_otp_and_refresh_token\" || true && npx prisma generate && npx prisma migrate deploy && npx prisma db seed && (node dist/main.js || node dist/src/main.js || (ls -R dist && exit 1))",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@getbrevo/brevo": "^3.0.1",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^6.19.2",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^2.9.0",
    "compression": "^1.8.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.19.2",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.9.2",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/bcrypt": "^5.0.2",
    "@types/buffer-to-stream": "^1.0.3",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.7",
    "@types/nodemailer": "^7.0.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript-eslint": "^8.54.0"
  },
  "prisma": {
    "seed": "npx ts-node --transpile-only prisma/seed.ts"
  },
  "overrides": {
    "lodash": "^4.17.21",
    "tar": "^7.4.3",
    "@nestjs/config": {
      "lodash": "^4.17.21"
    },
    "@nestjs/swagger": {
      "lodash": "^4.17.21"
    }
  }
}